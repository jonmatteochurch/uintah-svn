<?xml version='1.0' encoding='ISO-8859-1' ?>
<!-- Silicon - Properties from https://www.azom.com/properties.aspx?ArticleID=920 but with the "rigid" constitutive model, no stress is computed -->
<Uintah_Include>
    <density>2300.0</density>
    <constitutive_model type="rigid">
        <bulk_modulus> 1.5e9</bulk_modulus>
        <shear_modulus> 0.02e9</shear_modulus>
    </constitutive_model>
    <thermal_conductivity>1.0</thermal_conductivity>
    <specific_heat>1000.</specific_heat>
</Uintah_Include>
